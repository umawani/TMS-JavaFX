<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER"
      styleClass="background" spacing="10" prefHeight="300.0" prefWidth="350.0"
      fx:controller="main.controller.ErrorController"
>
   
    <Label styleClass="label-h2" text="Form Validation Errors" />

    <Text fx:id="errorText" styleClass="text-out" />

    <HBox alignment="CENTER">
        <Button styleClass="button" text="Okay" onAction="#onClose" />
    </HBox>

    <stylesheets>
        <URL value="@tmsfx.css"/>
    </stylesheets>
    
</VBox>

